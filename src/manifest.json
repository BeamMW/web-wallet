{
  "manifest_version": 2,
  "name": "Beam Web Wallet (Masternet)",
  "short_name": "Beam Web Wallet (Masternet)",
  "description": "Beam Web Wallet (Masternet)",
  "version": "1.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "assets/chrome-extention-icon-16x16.png",
    "48": "assets/chrome-extention-icon-48x48.png",
    "128": "assets/chrome-extention-icon-128x128.png"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
